# Grafana Environment Variables
# Copy this file to .env and fill in the values

# Grafana Admin Credentials
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your_secure_grafana_password_here
GRAFANA_SECRET_KEY=your_random_secret_key_here

# PostgreSQL Connection (for Grafana database - optional)
# GRAFANA_DB_PASSWORD=your_grafana_db_password_here

# Odoo Database Connection (for dashboards)
POSTGRES_PASSWORD=your_odoo_postgres_password_here

# Supabase Connection (if using)
# SUPABASE_PASSWORD=your_supabase_password_here

# SMTP Configuration (for alerting - optional)
# SMTP_USER=your-email@gmail.com
# SMTP_PASSWORD=your-app-specific-password
