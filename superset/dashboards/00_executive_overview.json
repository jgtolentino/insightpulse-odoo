        "order_desc": true,
        "color_scheme": "supersetColors",
        "show_legend": false,
        "rich_tooltip": true,
        "y_axis_format": "$,.0f",
        "x_axis_title": "Revenue",
        "y_axis_title": "Product",
        "orientation": "horizontal",
        "show_value": true
      },
      "cache_timeout": 3600,
      "description": "Top 10 products ranked by total revenue"
    },
    {
      "slice_name": "Revenue by Customer Segment",
      "viz_type": "pie",
      "params": {
        "datasource": "3__table",
        "viz_type": "pie",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "SUM(amount_total)",
          "label": "Revenue"
        },
        "groupby": [
          "customer_segment"
        ],
        "adhoc_filters": [],
        "color_scheme": "supersetColors",
        "show_legend": true,
        "show_labels": true,
        "labels_outside": true,
        "label_type": "key_percent_value",
        "donut": true,
        "innerRadius": 50,
        "outerRadius": 70
      },
      "cache_timeout": 3600,
      "description": "Revenue distribution across customer segments"
    },
    {
      "slice_name": "MRR/ARR Trend",
      "viz_type": "echarts_area",
      "params": {
        "datasource": "1__table",
        "viz_type": "echarts_area",
        "x_axis": "sale_date",
        "time_grain_sqla": "P1M",
        "metrics": [
          {
            "expressionType": "SQL",
            "sqlExpression": "SUM(confirmed_revenue)",
            "label": "MRR"
          },
          {
            "expressionType": "SQL",
            "sqlExpression": "SUM(confirmed_revenue) * 12",
            "label": "ARR"
          }
        ],
        "adhoc_filters": [],
        "groupby": [],
        "color_scheme": "supersetColors",
        "show_legend": true,
        "rich_tooltip": true,
        "stack": false,
        "area": true,
        "opacity": 0.7,
        "markerEnabled": true,
        "markerSize": 6,
        "zoomable": true,
        "y_axis_format": "$,.0f"
      },
      "cache_timeout": 3600,
      "description": "Monthly recurring revenue and annualized recurring revenue trends"
    },
    {
      "slice_name": "Customer Acquisition",
      "viz_type": "echarts_timeseries_line",
      "params": {
        "datasource": "4__table",
        "viz_type": "echarts_timeseries_line",
        "x_axis": "first_order_date",
        "time_grain_sqla": "P1M",
        "metrics": [
          {
            "expressionType": "SQL",
            "sqlExpression": "COUNT(DISTINCT customer_key)",
            "label": "New Customers"
          },
          {
            "expressionType": "SQL",
            "sqlExpression": "SUM(lifetime_value)",
            "label": "Total LTV"
          }
        ],
        "adhoc_filters": [],
        "groupby": [],
        "color_scheme": "supersetColors",
        "show_legend": true,
        "rich_tooltip": true,
        "show_markers": true,
        "markerSize": 6,
        "zoomable": true,
        "yAxisIndex": 0,
        "y_axis_format": ",d",
        "y_axis_2_format": "$,.0f"
      },
      "cache_timeout": 3600,
      "description": "Monthly customer acquisition with lifetime value"
    }
