  "charts": [
    {
      "slice_name": "Revenue KPI",
      "viz_type": "big_number_total",
      "params": {
        "datasource": "1__table",
        "viz_type": "big_number_total",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "SUM(confirmed_revenue)",
          "label": "Total Revenue"
        },
        "adhoc_filters": [],
        "header_font_size": 0.3,
        "subheader_font_size": 0.15,
        "y_axis_format": "$,.0f",
        "time_format": "smart_date",
        "color_picker": {
          "r": 25,
          "g": 118,
          "b": 210,
          "a": 1
        }
      },
      "cache_timeout": 300,
      "description": "Total confirmed revenue with trend indicator"
    },
    {
      "slice_name": "Orders KPI",
