        "subheader_font_size": 0.15,
        "y_axis_format": ",d",
        "time_format": "smart_date",
        "color_picker": {
          "r": 67,
          "g": 160,
          "b": 71,
          "a": 1
        }
      },
      "cache_timeout": 300,
      "description": "Number of active customers"
