    {
      "slice_name": "Margin KPI",
      "viz_type": "big_number_total",
      "params": {
        "datasource": "2__table",
        "viz_type": "big_number_total",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "AVG(avg_margin_percent)",
          "label": "Average Margin %"
        },
        "adhoc_filters": [],
        "header_font_size": 0.3,
        "subheader_font_size": 0.15,
        "y_axis_format": ".1%",
        "time_format": "smart_date",
        "color_picker": {
          "r": 255,
          "g": 160,
          "b": 0,
          "a": 1
        }
      },
      "cache_timeout": 300,
      "description": "Average profit margin percentage"
    },
    {
