  "charts": [
    {
      "slice_name": "Pipeline Value KPI",
      "viz_type": "big_number_total",
      "params": {
        "datasource": "7__table",
        "viz_type": "big_number_total",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "SUM(CASE WHEN state IN ('draft', 'sent') THEN amount_total ELSE 0 END)",
          "label": "Pipeline Value"
        },
        "adhoc_filters": [],
        "header_font_size": 0.3,
        "subheader_font_size": 0.15,
        "y_axis_format": "$,.0f",
        "time_format": "smart_date",
        "color_picker": {
          "r": 25,
          "g": 118,
          "b": 210,
          "a": 1
        }
      },
      "cache_timeout": 600,
      "description": "Total value in sales pipeline"
    },
    {
      "slice_name": "Closed Won KPI",
      "viz_type": "big_number_total",
      "params": {
        "datasource": "7__table",
        "viz_type": "big_number_total",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "SUM(CASE WHEN state IN ('sale', 'done') THEN amount_total ELSE 0 END)",
          "label": "Closed Won"
        },
        "adhoc_filters": [],
        "header_font_size": 0.3,
        "subheader_font_size": 0.15,
        "y_axis_format": "$,.0f",
        "time_format": "smart_date",
        "color_picker": {
          "r": 67,
          "g": 160,
          "b": 71,
          "a": 1
        }
      },
      "cache_timeout": 600,
      "description": "Total value of closed won deals"
    },
    {
      "slice_name": "Win Rate KPI",
      "viz_type": "big_number_total",
      "params": {
        "datasource": "7__table",
        "viz_type": "big_number_total",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "COUNT(CASE WHEN state IN ('sale', 'done') THEN 1 END)::FLOAT / NULLIF(COUNT(CASE WHEN state != 'cancel' THEN 1 END), 0) * 100",
          "label": "Win Rate %"
        },
        "adhoc_filters": [],
        "header_font_size": 0.3,
        "subheader_font_size": 0.15,
        "y_axis_format": ".1f",
        "time_format": "smart_date",
        "color_picker": {
          "r": 67,
          "g": 160,
          "b": 71,
          "a": 1
        }
      },
      "cache_timeout": 600,
      "description": "Percentage of won deals"
    },
    {
      "slice_name": "Average Deal Size KPI",
      "viz_type": "big_number_total",
      "params": {
        "datasource": "7__table",
        "viz_type": "big_number_total",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "AVG(CASE WHEN state IN ('sale', 'done') THEN amount_total END)",
          "label": "Avg Deal Size"
        },
        "adhoc_filters": [],
        "header_font_size": 0.3,
        "subheader_font_size": 0.15,
        "y_axis_format": "$,.0f",
        "time_format": "smart_date",
        "color_picker": {
          "r": 25,
          "g": 118,
          "b": 210,
          "a": 1
        }
      },
      "cache_timeout": 600,
      "description": "Average deal size for closed won deals"
    },
    {
      "slice_name": "Quota Attainment KPI",
      "viz_type": "big_number_total",
      "params": {
        "datasource": "7__table",
        "viz_type": "big_number_total",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "SUM(CASE WHEN state IN ('sale', 'done') THEN amount_total ELSE 0 END) / NULLIF(SUM(sales_quota), 0) * 100",
          "label": "Quota Attainment %"
        },
        "adhoc_filters": [],
        "header_font_size": 0.3,
        "subheader_font_size": 0.15,
        "y_axis_format": ".1f",
        "time_format": "smart_date",
        "color_picker": {
          "r": 67,
          "g": 160,
          "b": 71,
          "a": 1
        }
      },
      "cache_timeout": 600,
      "description": "Percentage of quota achieved"
    },
    {
      "slice_name": "Sales Funnel",
      "viz_type": "funnel",
      "params": {
        "datasource": "7__table",
        "viz_type": "funnel",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "SUM(amount_total)",
          "label": "Total Value"
        },
        "groupby": [
          "state"
        ],
        "adhoc_filters": [
          {
            "clause": "WHERE",
            "expressionType": "SQL",
            "sqlExpression": "state != 'cancel'"
          }
        ],
        "color_scheme": "supersetColors",
        "label_type": "key_percent_value",
        "show_legend": true
      },
      "cache_timeout": 600,
      "description": "Sales pipeline funnel by stage"
    },
    {
      "slice_name": "Revenue by Salesperson",
      "viz_type": "echarts_timeseries_bar",
      "params": {
        "datasource": "7__table",
        "viz_type": "echarts_timeseries_bar",
        "metrics": [
          {
            "expressionType": "SQL",
            "sqlExpression": "SUM(CASE WHEN state IN ('sale', 'done') THEN amount_total ELSE 0 END)",
            "label": "Revenue"
          }
        ],
        "groupby": [
          "salesperson_name"
        ],
        "adhoc_filters": [],
        "row_limit": 10,
        "order_desc": true,
        "color_scheme": "supersetColors",
        "show_legend": false,
        "rich_tooltip": true,
        "y_axis_format": "$,.0f",
        "x_axis_title": "Revenue",
        "y_axis_title": "Salesperson",
        "orientation": "horizontal",
        "show_value": true
      },
      "cache_timeout": 600,
      "description": "Top 10 salespeople by revenue"
    },
    {
      "slice_name": "Monthly Sales Trend vs Target",
      "viz_type": "echarts_timeseries_bar",
      "params": {
        "datasource": "7__table",
        "viz_type": "echarts_timeseries_bar",
        "x_axis": "order_date",
        "time_grain_sqla": "P1M",
        "metrics": [
          {
            "expressionType": "SQL",
            "sqlExpression": "SUM(CASE WHEN state IN ('sale', 'done') THEN amount_total ELSE 0 END)",
            "label": "Actual Sales"
          },
          {
            "expressionType": "SQL",
            "sqlExpression": "SUM(sales_quota)",
            "label": "Target"
          }
        ],
        "adhoc_filters": [],
        "groupby": [],
        "color_scheme": "supersetColors",
        "show_legend": true,
        "rich_tooltip": true,
        "y_axis_format": "$,.0f",
        "show_value": false,
        "zoomable": true
      },
      "cache_timeout": 600,
      "description": "Monthly sales performance vs target"
    },
    {
      "slice_name": "Top Customers by LTV",
      "viz_type": "table",
      "params": {
        "datasource": "8__table",
        "viz_type": "table",
        "groupby": [
          "customer_name"
        ],
        "metrics": [
          {
            "expressionType": "SQL",
            "sqlExpression": "SUM(lifetime_value)",
            "label": "LTV"
          },
          {
            "expressionType": "SQL",
            "sqlExpression": "SUM(total_orders)",
            "label": "Orders"
          },
          {
            "expressionType": "SQL",
            "sqlExpression": "AVG(avg_order_value)",
            "label": "Avg Value"
          },
          {
            "expressionType": "SQL",
            "sqlExpression": "MAX(days_since_last_order)",
            "label": "Days Since Last"
          }
        ],
        "adhoc_filters": [],
        "row_limit": 20,
        "order_desc": true,
        "show_cell_bars": true,
        "table_timestamp_format": "smart_date",
        "page_length": 10,
        "include_search": true,
        "align_pn": false,
        "color_pn": true,
        "conditional_formatting": [
          {
            "column": "Days Since Last",
            "operator": "<=",
            "targetValue": 30,
            "colorScheme": "#43A047"
          },
          {
            "column": "Days Since Last",
            "operator": ">",
            "targetValue": 90,
            "colorScheme": "#D32F2F"
          }
        ]
      },
      "cache_timeout": 3600,
      "description": "Top 20 customers by lifetime value"
    },
    {
      "slice_name": "Sales Cycle Analysis",
      "viz_type": "box_plot",
      "params": {
        "datasource": "7__table",
        "viz_type": "box_plot",
        "metrics": [
          {
            "expressionType": "SQL",
            "sqlExpression": "days_to_close",
            "label": "Days to Close"
          }
        ],
        "groupby": [
          "sales_team_name"
        ],
        "adhoc_filters": [
          {
            "clause": "WHERE",
            "expressionType": "SQL",
            "sqlExpression": "state IN ('sale', 'done') AND days_to_close IS NOT NULL"
          }
        ],
        "color_scheme": "supersetColors",
        "whisker_options": "Min/max (no outliers)",
        "x_ticks_layout": "auto"
      },
      "cache_timeout": 3600,
      "description": "Sales cycle time distribution by team"
    }
  ],
