  "charts": [
    {
      "slice_name": "Order Cycle Time KPI",
      "viz_type": "big_number_total",
      "params": {
        "datasource": "7__table",
        "viz_type": "big_number_total",
        "metric": {
          "expressionType": "SQL",
          "sqlExpression": "AVG(days_to_deliver)",
          "label": "Avg Order Cycle (Days)"
        },
        "adhoc_filters": [
          {
            "clause": "WHERE",
            "expressionType": "SQL",
            "sqlExpression": "state = 'done' AND days_to_deliver IS NOT NULL"
          }
        ],
        "header_font_size": 0.3,
        "subheader_font_size": 0.15,
        "y_axis_format": ".1f",
        "time_format": "smart_date",
        "color_picker": {
          "r": 25,
          "g": 118,
          "b": 210,
          "a": 1
        }
      },
      "cache_timeout": 1800,
      "description": "Average order fulfillment cycle time"
    },
