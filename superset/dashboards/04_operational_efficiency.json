        "metrics": [
          {
            "expressionType": "SQL",
            "sqlExpression": "AVG(avg_days)",
            "label": "Avg Days"
          },
          {
            "expressionType": "SQL",
            "sqlExpression": "AVG(std_dev)",
            "label": "Std Dev"
          },
          {
            "expressionType": "SQL",
            "sqlExpression": "MAX(max_days)",
            "label": "Max Days"
          }
        ],
        "adhoc_filters": [],
        "row_limit": 20,
        "order_desc": true,
        "show_cell_bars": true,
        "table_timestamp_format": "smart_date",
        "page_length": 10,
        "include_search": true,
        "align_pn": false,
        "color_pn": true,
        "conditional_formatting": [
          {
            "column": "Avg Days",
            "operator": ">",
            "targetValue": 15,
            "colorScheme": "#D32F2F"
          },
          {
            "column": "Avg Days",
            "operator": "<=",
            "targetValue": 10,
            "colorScheme": "#43A047"
          }
        ]
