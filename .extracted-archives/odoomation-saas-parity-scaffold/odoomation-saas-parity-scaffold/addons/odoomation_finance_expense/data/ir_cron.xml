<odoo>
  <record id="ir_cron_expense_cleanup" model="ir.cron">
    <field name="name">Expense Cleanup</field>
    <field name="model_id" ref="model_odoomation_expense_atomic"/>
    <field name="state">code</field>
    <field name="code">model.search([('state','=','draft')]).unlink()</field>
